.authorline input.firstnames:livesearch-searchbox(kssAttr(firstnames)){
    evt-livesearch-searchbox-submit: true;
    evt-livesearch-searchbox-complete: true;
    evt-livesearch-searchbox-rowClass: authorline;
    evt-livesearch-searchbox-highlightId: LSKacHighlight;
    evt-livesearch-searchbox-useSeparator: true;
    action-server: getAuthorSearchResults;
    getAuthorSearchResults-q: pass(q);
    getAuthorSearchResults-limit: 5;
}
