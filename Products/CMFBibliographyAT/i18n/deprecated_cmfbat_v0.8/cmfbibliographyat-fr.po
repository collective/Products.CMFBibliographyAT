#: bibtex_types/ArticleReference.py
msgid  "Article Reference"
msgstr "Référence d'article"

#: bibtex_types/ArticleReference.py
msgid "label_source_journal"
msgstr "Journal"

#: bibtex_types/ArticleReference.py
msgid "help_source_journal"
msgstr "Le nom du journal dont la publication est issue."

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "label_source_volume"
msgstr "Volume"

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "help_source_volume"
msgstr "Le numéro de volume du journal ou du livre multi-volumes."

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "label_source_number"
msgstr "Numéro"

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "help_source_number"
msgstr "Le numéro du journal, du magazine, ou du rapport technique. Ce numéro est habituellement identifié par son volume et numéro; l'organisation qui émet un rapport technique lui donne habituellement un numéro."

#: bibtex_types/ArticleReference.py
#: bibtex_types/InbookReference.py
msgid "label_source_pages"
msgstr "Pages"

#: bibtex_types/ArticleReference.py
#: bibtex_types/InbookReference.py
msgid "help_source_pages"
msgstr "Un numéro de page ou un intervalle de nombres tels que '42 -- 111'; Vous pouvez également avoir plusieurs numéros séparés par des virgules: '7,41,73 -- 97' (Les modèles standard convertissent un tiret simple en double)."

#: bibtex_types/ArticleReference.py
msgid "label_source_pmid"
msgstr "PMID"

#: bibtex_types/ArticleReference.py
msgid "help_source_pmid"
msgstr "Numéro d'identification dans la base de données PubMed."

#: bibtex_types/ArticleReference.py
msgid "label_editor_flag"
msgstr "Rédacteur"

#: bibtex_types/ArticleReference.py
msgid "help_editor_flag"
msgstr "Cochez cette case si les auteurs de l'ouvrage sont bien les rédacteurs de celui-ci."

#: bibtex_types/BookletReference.py
msgid  "Booklet Reference"
msgstr "Référence de facsicule"

#: bibtex_types/BookletReference.py
#: bibtex_types/MiscReference.py
msgid "label_source_howpublished"
msgstr "Moyen de publication"

#: bibtex_types/BookletReference.py
#: bibtex_types/MiscReference.py
msgid "help_source_howpublished"
msgstr "Comment a été publié l'ouvrage. Destiné à des publications sans éditeur. Par exemple, un 'Rapport d'institution'."

#: bibtex_types/BookReference.py
msgid  "Book Reference"
msgstr "Référence de livre"

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "label_source_publisher"
msgstr "Éditeur ('publisher')"

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "help_source_publisher"
msgstr "Le nom de l'éditeur."

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ManualReference.py
#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "label_source_address"
msgstr "Adresse"

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ManualReference.py
#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "help_source_address"
msgstr "L'adresse de l'éditeur. Pour les maisons d'édition principales, la ville suffit. Pour de petits éditeurs, vous pouvez aider le lecteur en donnant l'adresse complète."

#: bibtex_types/BookReference.py
#: bibtex_types/ManualReference.py
msgid "label_source_edition"
msgstr "Edition"

#: bibtex_types/BookReference.py
#: bibtex_types/ManualReference.py
msgid "help_source_edition"
msgstr "L'édition d'un livre - par exemple, 'seconde'."

#: bibtex_types/InbookReference.py
msgid  "Inbook Reference"
msgstr "Référence d'une partie de livre"

#: bibtex_types/InbookReference.py
msgid "label_source_booktitle"
msgstr "Titre du livre"

#: bibtex_types/InbookReference.py
msgid "help_source_booktitle"
msgstr "Titre du livre dont la partie est citée."

#: bibtex_types/InbookReference.py
msgid "label_source_editor"
msgstr "Éditeur ('editor')"

#: bibtex_types/InbookReference.py
msgid "help_source_editor"
msgstr "Nom(s) de l'éditeur ou des éditeurs. S'il existe aussi un champ 'author', alors le champ 'editor' indique l'éditeur du livre ou de la collection qui contient la référence."

#: bibtex_types/InbookReference.py
msgid "label_source_chapter"
msgstr "Chapitre"

#: bibtex_types/InbookReference.py
msgid "help_source_chapter"
msgstr "Le numéro du chapitre."

#: bibtex_types/IncollectionReference.py
msgid  "Incollection Reference"
msgstr "Référence d'une partie de livre avec son titre"

#: bibtex_types/InproceedingsReference.py
msgid  "Inproceedings Reference"
msgstr "Référence d'un article dans les actes d'une conférence"

#: bibtex_types/ManualReference.py
msgid  "Manual Reference"
msgstr "Référence de manuel"

#: bibtex_types/ManualReference.py
#: bibtex_types/ProceedingsReference.py
msgid "label_source_organization"
msgstr "Organisation"

#: bibtex_types/ManualReference.py
#: bibtex_types/ProceedingsReference.py
msgid "help_source_organization"
msgstr "L'organisation commanditant une conférence."

#: bibtex_types/MasterthesisReference.py
msgid  "Masterthesis Reference"
msgstr "Thèse"

#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
msgid "label_source_school"
msgstr "École"

#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
msgid "help_source_school"
msgstr "Le nom de l'établissement scolaire où la thèse a été écrite."

#: bibtex_types/MiscReference.py
msgid  "Misc Reference"
msgstr "Référence diverse"

#: bibtex_types/PhdthesisReference.py
msgid  "Phdthesis Reference"
msgstr "Référence de Thèse de troisième cycle"

#: bibtex_types/PreprintReference.py
msgid  "Preprint Reference"
msgstr "Référence de prépublication"

#: bibtex_types/PreprintReference.py
msgid "label_source_preprint_server"
msgstr "Serveur de prépublication"

#: bibtex_types/PreprintReference.py
msgid "help_source_preprint_server"
msgstr "Le nom du serveur de prépublication."

#: bibtex_types/ProceedingsReference.py
msgid  "Proceedings Reference"
msgstr "Référence des actes d'une conférence"

#: bibtex_types/ProceedingsReference.py
msgid "label_source_series"
msgstr "Série"

#: bibtex_types/ProceedingsReference.py
msgid "help_source_series"
msgstr "Le nom d'une série ou d'un ensemble de livres. Lorsqu'on cite un livre entier, le champ 'titre' donne le titre et un champ optionnel 'série' donne le nom de la série dans lequel le livre est publié."

#: ./bibtex_types/TechreportReference.py
msgid  "label_source_institution"
msgstr "Institution"

#: ./bibtex_types/TechreportReference.py
msgid  "help_source_institution"
msgstr "L'institution qui a publié l'ouvrage."

#: ./bibtex_types/TechreportReference.py
msgid  "label_source_type"
msgstr "Type"

#: ./bibtex_types/TechreportReference.py
msgid  "help_source_type"
msgstr "Le type du rapport technique - par exemple, 'Notes de recherche'."

#: ./bibtex_types/UnpublishedReference.py
msgid  "Unpublished Reference"
msgstr "Référence d'un document pas spécialement édité"

#: ./bibtex_types/WebpublishedReference.py
msgid  "Webpublished Reference"
msgstr "Référence d'un document publié sur internet"

