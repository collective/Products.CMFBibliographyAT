msgid  "Article Reference"
msgstr "Artigo"

#: bibtex_types/ArticleReference.py
msgid "label_source_journal"
msgstr "Publicação periódica"

#: bibtex_types/ArticleReference.py
msgid "help_source_journal"
msgstr "Nome de publicação periódica (eventualmente na forma abreviada normalizada)."

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "label_source_volume"
msgstr "Volume"

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "help_source_volume"
msgstr "O número de volume de uma publicação periódica ou de uma obra em vários volumes."

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "label_source_number"
msgstr "Número"

#: bibtex_types/ArticleReference.py
#: bibtex_types/BookReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "help_source_number"
msgstr "O número de um jornal, revista ou relatório técnico.  Uma edição de uma publicação periódica é habitualmente identificada pelo seu volume e número; a organização que edita um relatório técnico dá-lhe habitualmente um número."

#: bibtex_types/ArticleReference.py
#: bibtex_types/InbookReference.py
msgid "label_source_pages"
msgstr "Páginas"

#: bibtex_types/ArticleReference.py
#: bibtex_types/InbookReference.py
msgid "help_source_pages"
msgstr "Um número de página ou sequência de números como '42--111'; poderá também indicar vários destes, separando-os com vírgulas: '7,41,73--97'. Os estilos-padrão convertem um traço único (-) num traço duplo (--)."

#: bibtex_types/ArticleReference.py
msgid "label_source_pmid"
msgstr "PMID"

#: bibtex_types/ArticleReference.py
msgid "help_source_pmid"
msgstr "Número da referência na base de dados PubMed."

#: bibtex_types/ArticleReference.py
msgid "label_editor_flag"
msgstr "Indicação de editor"

#: bibtex_types/ArticleReference.py
msgid "help_editor_flag"
msgstr "Indique aqui se o(s) autor(es) especificado(s) acima é(são) o(s) editor(es) do livro."

#: bibtex_types/BookletReference.py
msgid  "Booklet Reference"
msgstr "Folheto"

#: bibtex_types/BookletReference.py
#: bibtex_types/MiscReference.py
msgid "label_source_howpublished"
msgstr "Publicado como"

#: bibtex_types/BookletReference.py
#: bibtex_types/MiscReference.py
msgid "help_source_howpublished"
msgstr "Como a obra foi publicada (principalmente para publicações sem editor).  Por exemplo, um 'Relatório de um instituto'."

#: bibtex_types/BookReference.py
msgid  "Book Reference"
msgstr "Livro"

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "label_source_publisher"
msgstr "Editor"

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ProceedingsReference.py
msgid "help_source_publisher"
msgstr "O nome da pessoa ou organização responsável pela produção e disseminação."

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ManualReference.py
#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "label_source_address"
msgstr "Endereço"

#: bibtex_types/BookReference.py
#: bibtex_types/InbookReference.py
#: bibtex_types/ManualReference.py
#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
#: bibtex_types/ProceedingsReference.py
#: bibtex_types/TechreportReference.py
msgid "help_source_address"
msgstr "Endereço do editor.  Para as principais casas editoras, apenas é indicada a cidade.  Para pequenos editores, poderá ajudar o leitor indicando o endereço completo."

#: bibtex_types/BookReference.py
#: bibtex_types/ManualReference.py
msgid "label_source_edition"
msgstr "Edição"

#: bibtex_types/BookReference.py
#: bibtex_types/ManualReference.py
msgid "help_source_edition"
msgstr "A edição de um livro - por exemplo, 'segunda'."

#: bibtex_types/InbookReference.py
msgid  "Inbook Reference"
msgstr "Parte de livro"

#: bibtex_types/InbookReference.py
msgid "label_source_booktitle"
msgstr "Título do livro"

#: bibtex_types/InbookReference.py
msgid "help_source_booktitle"
msgstr "Título do livro cuja parte é citada."

#: bibtex_types/InbookReference.py
msgid "label_source_editor"
msgstr "Editor"

#: bibtex_types/InbookReference.py
msgid "help_source_editor"
msgstr "Nome(s) de editor(es).  Se também existe um campo 'autor', o campo 'editor' indica o editor do livro ou colecção em que a referência aparece."

#: bibtex_types/InbookReference.py
msgid "label_source_chapter"
msgstr "Capítulo"

#: bibtex_types/InbookReference.py
msgid "help_source_chapter"
msgstr "Um número de capítulo."

#: bibtex_types/IncollectionReference.py
msgid  "Incollection Reference"
msgstr "Parte de livro com título próprio"

#: bibtex_types/InproceedingsReference.py
msgid  "Inproceedings Reference"
msgstr "Parte de acta de encontro."

#: bibtex_types/ManualReference.py
msgid  "Manual Reference"
msgstr "Manual"

#: bibtex_types/ManualReference.py
#: bibtex_types/ProceedingsReference.py
msgid "label_source_organization"
msgstr "Organização"

#: bibtex_types/ManualReference.py
#: bibtex_types/ProceedingsReference.py
msgid "help_source_organization"
msgstr "A organização responsável (por exemplo, por uma conferência)."

#: bibtex_types/MasterthesisReference.py
msgid  "Masterthesis Reference"
msgstr "Tese de mestrado"

#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
msgid "label_source_school"
msgstr "Instituição académica"

#: bibtex_types/MasterthesisReference.py
#: bibtex_types/PhdthesisReference.py
msgid "help_source_school"
msgstr "O nome da instituição académica onde a tese foi escrita."

#: bibtex_types/MiscReference.py
msgid  "Misc Reference"
msgstr "Referência diversa"

#: bibtex_types/PhdthesisReference.py
msgid  "Phdthesis Reference"
msgstr "Tese de doutoramento"

#: bibtex_types/PreprintReference.py
msgid  "Preprint Reference"
msgstr "Pré-publicação"

#: bibtex_types/PreprintReference.py
msgid "label_source_preprint_server"
msgstr "Servidor de pré-publicações"

#: bibtex_types/PreprintReference.py
msgid "help_source_preprint_server"
msgstr "O nome do servidor de pré-publicações."

#: bibtex_types/ProceedingsReference.py
msgid  "Proceedings Reference"
msgstr "Acta de encontro"

#: bibtex_types/ProceedingsReference.py
msgid "label_source_series"
msgstr "Série"

#: bibtex_types/ProceedingsReference.py
msgid "help_source_series"
msgstr "O nome de uma série ou conjunto de livros.  Ao citar um livro completo, o campo 'título' indica o seu título e um campo opcional 'série' indica o nome de uma série na qual o livro está publicado."

#: ./bibtex_types/TechreportReference.py
msgid  "label_source_institution"
msgstr "Instituição"

#: ./bibtex_types/TechreportReference.py
msgid  "help_source_institution"
msgstr "A instituição que publicou a obra."

#: ./bibtex_types/TechreportReference.py
msgid  "label_source_type"
msgstr "Tipo"

#: ./bibtex_types/TechreportReference.py
msgid  "help_source_type"
msgstr "O tipo de um relatório técnico - por exemplo, 'Nota de Investigação'."

#: ./bibtex_types/UnpublishedReference.py
msgid  "Unpublished Reference"
msgstr "Documento não publicado"

#: ./bibtex_types/WebpublishedReference.py
msgid  "Webpublished Reference"
msgstr "Publicado na WWW"

